                            adTitle: res.data[5][0].name,
                            adPos: 5
                        });
                    });
                                    } else if (res.data[1]) {
                                        require.async(['wiki-lemma:widget/promotion/vbaike/vbaike'], function(vbaike) {
                        vbaike(res.data[1]);
                        for(var i in res.data[1]) {
